		"bulma": "^1.0.3",
		"bulma-switch": "^2.0.4",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"material-icons": "^1.13.13",
